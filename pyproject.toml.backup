authors = [
    {name = "Your Name",email = "you@example.com"}
]
readme = "README.md"
requires-python = ">=3.11,<4.0"

dependencies = [
    "langchain (>=0.3.24,<0.4.0)",
    "langchain-openai (>=0.3.14,<0.4.0)",
    "python-dotenv (>=1.1.0,<2.0.0)",
    "langchain-cli (>=0.0.36,<0.0.37)",
    "langgraph (>=0.3.33,<0.4.0)",
    "dash (>=3.0.4,<4.0.0)",
    "pandas (>=2.2.3,<3.0.0)",
    "dash-chat (>=0.2.0,<0.3.0)"
]

[tool.poetry]
name = "portubot"
version = "0.1.0"
description = "An application to learn portuguese"
authors = ["Your Name <you@example.com>"]
readme = "README.md"
 



[build-system]
requires = ["poetry-core>=2.0.0,<3.0.0"]
